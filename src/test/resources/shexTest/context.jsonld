    "entries": {"@id": "mf:entries", "@container": "@list", "@type": "@id"},
    "extensionResults": {"@id": "mf:extensionResults", "@type": "@id", "@container": "@list"},
    "include": {"@id": "mf:include", "@type": "@id", "@container": "@list"},

    "action": {"@id": "mf:action", "@type": "@id"},
    "data": {"@id": "sx:data", "@type": "@id"},
    "extension": {"@id": "mf:extension", "@type": "@id"},
    "focus": {"@id": "sx:focus", "@type": "@id"},
    "json": {"@id": "sx:json", "@type": "@id"},
    "result": {"@id": "mf:result", "@type": "@id"},
    "schema": {"@id": "sx:schema", "@type": "@id"},
    "shape": {"@id": "sx:shape", "@type": "@id"},
    "shex": {"@id": "sx:shex", "@type": "@id"},
    "status": {"@id": "mf:status", "@type": "@vocab"},
